{
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "prettier"
    ],
    "ignorePatterns": ["ecosystem.config.js"],
    "plugins": ["prettier"],
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "import/no-unused-modules": ["error", { "unusedExports": true }],
      "import/order": [
        "error",
        {
          "groups": [
            "external",
            "builtin",
            "internal",
            "sibling",
            "parent",
            "index"
          ],
          "newlines-between": "always"
        }
      ]
    }
  }
  